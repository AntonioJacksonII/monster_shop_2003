<h1>Enter Log in Credentials</h1>


<%= form_tag "/login", method: :post, class: "navbar-form navbar-left search-form" do %>
  <%= text_field_tag(
      :email,
      params[:email],
      placeholder: "Email") %>
  <%= password_field_tag(:password,
                          params[:password],
                          placeholder: "Password") %>
  <%= submit_tag("Log In") %>
<% end %>
